#include <stdio.h>

void decima12bainary(char num);

int main (void){
  char x;
  
  scanf("%d",&x);

  decima12bainary(x);

 return 0;
}

void decima12bainary(char num){
  char y;
  char array[8];
  char i;
  y=0;
  

 if(num!=0){
    
    for(i=0;i<8;i++){

    array[i] = num%2;
    num=num/2;
    y += y;
    }
    y = y-1;
    while(y>=0){
      printf("%d",array[i]);
	}
  }
  else{
    printf("0");
  }
  printf("\n");
}
